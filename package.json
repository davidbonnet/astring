{
  "name": "astring",
  "version": "0.6.1",
  "description": "JavaScript code generator from an ESTree-compliant AST.",
  "main": "./dist/astring",
  "bin": {
    "astring": "./bin/astring"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "full-test": "node test/scripts.js",
    "prepublish": "npm run build",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "start": "babel --watch src -d dist",
    "benchmark": "node ./test/benchmark.js"
  },
  "keywords": [
    "ast",
    "code generator",
    "estree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidbonnet/astring.git"
  },
  "author": "David Bonnet <david@bonnet.cc>",
  "license": "MIT",
  "dependencies": {
    "string.prototype.repeat": "^0.2.0"
  },
  "devDependencies": {
    "acorn": "^3.0.4",
    "astravel": "^0.3.11",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-preset-es2015": "^6.6.0",
    "benchmark": "^2.0.0",
    "cross-env": "^2.0.0",
    "escodegen": "^1.6.1",
    "eslint": "^2.2.0",
    "esotope": "^1.4.5",
    "glob": "^7.0.0",
    "mocha": "^2.2.5",
    "modify-babel-preset": "^2.1.1",
    "source-map": "^0.5.3",
    "uglify-js": "^2.6.0"
  }
}
